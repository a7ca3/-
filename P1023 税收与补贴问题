#include<iostream>
#include<cstdio>
using namespace std;
int jia[1000000],liang[1000000];
int main()
{
	int a,b,c,d,zui,max1=0,max2=0,zui1,i=0;
	cin>>a>>b>>c;
	zui=(a-b)*c;
	while(jia[i]==0&&liang[i]==0)
	{
		i++;
		cin>>jia[i]>>liang[i];
		if(jia[i]==-1&&liang[i]==-1) break;
		if(max<jia[i])
		{
		    max1=jia[i];         
		    max2=liang[i];       
		}
		if((jia[i]-a)*liang[i]>zui)
		{
		    cout<<"NO SOLUTION"<<endl;
		    exit(0);
		}
	}
	cin>>d;
	while(zui1==0)
	{
		zui1=(max+1)*(max2-d);
		if(zui1>zui)
		{
			cout<<"NO SOLUTION"<<endl;
			exit(0); 
		}
	}
	cout<<a-b<<endl;
	return 0;
}
